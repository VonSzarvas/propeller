;LOAD "WALTR.LGO

MAKE "LEN 50

TO SQUARE :DIST
  REPEAT 4 [RIGHT 90 FORWARD :DIST]
END

TO HOUSE
  PD
  SQUARE :LEN
  RIGHT 30
  FORWARD :LEN
  RIGHT 120
  FORWARD :LEN
  PU
END

